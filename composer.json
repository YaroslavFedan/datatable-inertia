{
    "name": "dongrim/datatable-inertia",
    "description": "Easy build datatable for InertiaJS on Laravel",
    "keywords": [
        "datatable",
        "InertiaJS",
        "inertia",
        "laravel",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Fedan",
            "email": "yaroslav.fd@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "laravel/framework": "~6.0||^7.0||^8.0||^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dongrim\\DatatableInertia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dongrim\\DatatableInertia\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.24",
        "phpunit/phpunit": "9.5.*",
        "laravel/pint": "^0.2.3",
        "inertiajs/inertia-laravel": "^0.6.3",
        "laravel/legacy-factories": "1.x"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dongrim\\DatatableInertia\\DatatableInertiaServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}